{
  "name": "telegram_bot",
  "version": "1.0.0",
  "description": "telegram bot",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "worker": "nodemon src/worker.js",
    "lint": "eslint src/**/*.ts src/*.ts --fix",
    "format": "prettier --write src/*.ts src/**/*.ts",
    "build": "tsc -p tsconfig.json"
  },
  "husky": {
    "pre-commit": "yarn lint  && yarn format"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bmusuko/telegram_bot.git"
  },
  "keywords": [
    "telegram"
  ],
  "author": "bram musuko panjaitan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmusuko/telegram_bot/issues"
  },
  "homepage": "https://github.com/bmusuko/telegram_bot#readme",
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "amqplib": "^0.6.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "^0.0.1-security",
    "instagram-private-api": "^1.40.1",
    "moment": "^2.29.1",
    "node-telegram-bot-api": "^0.40.0",
    "nodemon": "^2.0.2",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "text-to-mp3": "^1.0.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.16",
    "@types/app-root-path": "^1.2.4",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/moment": "^2.13.0",
    "@types/node-telegram-bot-api": "^0.50.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0"
  }
